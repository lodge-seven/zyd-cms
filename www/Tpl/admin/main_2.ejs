			window.location.href = "/Tpl/admin/main.xhtml?target="+aaa.split('Tpl/')[1];
		}else{
			html_name(name)
			window.location.href = "/Tpl/admin/main.xhtml?nav="+html_name('nav')+'&target='+aaa.split('Tpl/')[1];
		}
	}

	/*[网址元素*/
	function html_name(name){
		html=window.location.search;
		arr_html=html.split("?");
		if(arr_html.length>1){
			arr_html=arr_html[1].split("&");

			for(i=0;i<arr_html.length;i++){
				arr_b=arr_html[i].split("=");
				if(arr_b[0]==name){
					return arr_b['1'];
					break;
				}

			}
			return '';
		}
		else
			return '';
	}

